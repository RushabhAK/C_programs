p102(b)
#include<stdio.h>
int main()
{
    int num,numb[100],count=0;
    printf("Enter the number:");
    scanf("%d",&num);
    printf("Enter the %d numbers",num);
    for(int i=0;i<num;i++)
    {
        scanf("%d",&numb[i]);
        for(int j=0;j<i;j++)
        {
            if(numb[i]==numb[j])
            {
                count++;
            }
        }
    }
    printf("%d duplicate numbers",count);
}



p103
#include<stdio.h>
int main()
{
    int num,numb[100],temp;
    printf("Enter the number:");
    scanf("%d",&num);
    printf("Enter the %d numbers\n",num);
    for(int i=0; i<num; i++)
    {
        scanf("%d",&numb[i]);
        for(int j=0; j<i; j++)
        {
            if(numb[i]>numb[j])
            {
             temp=numb[i];
             numb[i]=numb[j];
             numb[j]=temp;
            }

        }
    }
//    for(int k=0;k<num;k++)
//        printf("%d\n",numb[k]);
    printf("2nd biggest number=%d",numb[1]);
}

p104(a)
#include<stdio.h>
int pass_arr(int arr[],int num);
int main()
{
    int num;
    printf("Enter the size of array:");
    scanf("%d",&num);
    int arr[num];
    printf("Enetr the %d numbers:\n",num);
    for(int i=0; i<num; i++)
    {
        scanf("%d",&arr[i]);
    }
    printf("\nEnetered array is:\n");
    for(int i=0; i<num; i++)
    {
        printf("%d\t",arr[i]);
    }
    printf("\n");
    pass_arr(arr,num);
}

int pass_arr(int ar[],int nu)
{
     for(int i=0; i<nu; i++)
    {
        printf("passes number=%d\n",ar[i]);
    }
}


p104(b)
#include<stdio.h>
void pass_2d_arr(int ar[2][2]);
int main()
{
    int arr[2][2];
    printf("Enter the elements of array\n");
    for(int i=0; i<2; i++)
    {
        for(int j=0; j<2; j++)
        {
            scanf("%d",&arr[i][j]);
        }
    }
    pass_2d_arr(arr);
}
//////////////////////////////////////
void pass_2d_arr(int ar[2][2])
{
    printf("passed array elements\n");
    for(int i=0; i<2; i++)
    {
        for(int j=0; j<2; j++)
        {
            printf("%d\t",ar[i][j]);
        }
        printf("\n");
    }
}

p105(a)
#include<stdio.h>
#define N 50
int main()
{
    int arr1[N][N],arr2[N][N],res[N][N];
    printf("Enter the elements of 1st array:\n");
    for(int i=0; i<2; i++)
    {
        for(int j=0; j<2; j++)
        {
            scanf("%d",&arr1[i][j]);
        }
    }
    printf("firest matrix:\n");
    for(int i=0; i<2; i++)
    {
        for(int j=0; j<2; j++)
        {
            printf("%d\t",arr1[i][j]);
        }
        printf("\n");
    }

    printf("Enter the elements of 2st array:\n");
    for(int i=0; i<2; i++)
    {
        for(int j=0; j<2; j++)
        {
            scanf("%d",&arr2[i][j]);
        }
    }
    printf("second4 matrix:\n");
    for(int i=0; i<2; i++)
    {
        for(int j=0; j<2; j++)
        {
            printf("%d\t",arr2[i][j]);
        }
        printf("\n");
    }

    for(int i=0; i<2; i++)
    {
        for(int j=0; j<2; j++)
            res[i][j]=arr1[i][j]+arr2[i][j];
    }

    printf("Result matrix:\n");
    for(int i=0; i<2; i++)
    {
        for(int j=0; j<2; j++)
        {
            printf("%d\t",res[i][j]);
        }
        printf("\n");
    }
}


p105(b)
#include<stdio.h>
#define N 50
int main()
{
    int arr1[N][N],arr2[N][N],res[N][N];
    printf("Enter the elements of 1st array:\n");
    for(int i=0; i<2; i++)
    {
        for(int j=0; j<2; j++)
        {
            scanf("%d",&arr1[i][j]);
        }
    }
    printf("firest matrix:\n");
    for(int i=0; i<2; i++)
    {
        for(int j=0; j<2; j++)
        {
            printf("%d\t",arr1[i][j]);
        }
        printf("\n");
    }

    printf("Enter the elements of 2st array:\n");
    for(int i=0; i<2; i++)
    {
        for(int j=0; j<2; j++)
        {
            scanf("%d",&arr2[i][j]);
        }
    }
    printf("second4 matrix:\n");
    for(int i=0; i<2; i++)
    {
        for(int j=0; j<2; j++)
        {
            printf("%d\t",arr2[i][j]);
        }
        printf("\n");
    }

    for(int i=0; i<2; i++)
    {
        for(int j=0; j<2; j++)
            res[i][j]=arr1[i][j]-arr2[i][j];
    }

    printf("Result matrix:\n");
    for(int i=0; i<2; i++)
    {
        for(int j=0; j<2; j++)
        {
            printf("%d\t",res[i][j]);
        }
        printf("\n");
    }
}


p105(c)
#include<stdio.h>
#define N 50
int main()
{
    int arr1[N][N],arr2[N][N],res[N][N],sum;
    printf("Enter the elements of 1st array:\n");
    for(int i=0; i<2; i++)
    {
        for(int j=0; j<2; j++)
        {
            scanf("%d",&arr1[i][j]);
        }
    }
    printf("firest matrix:\n");
    for(int i=0; i<2; i++)
    {
        for(int j=0; j<2; j++)
        {
            printf("%d\t",arr1[i][j]);
        }
        printf("\n");
    }

    printf("Enter the elements of 2st array:\n");
    for(int i=0; i<2; i++)
    {
        for(int j=0; j<2; j++)
        {
            scanf("%d",&arr2[i][j]);
        }
    }
    printf("second4 matrix:\n");
    for(int i=0; i<2; i++)
    {
        for(int j=0; j<2; j++)
        {
            printf("%d\t",arr2[i][j]);
        }
        printf("\n");
    }
    for(int i=0; i<2; i++)
    {
        for(int j=0; j<2; j++)
        {
            sum=0;
            for(int k=0; k<2; k++)
            {
                sum=sum+arr1[i][k]*arr2[k][j];
            }
            res[i][j]=sum;
        }
    }

    printf("Result matrix:\n");
    for(int i=0; i<2; i++)
    {
        for(int j=0; j<2; j++)
        {
            printf("%d\t",res[i][j]);
        }
        printf("\n");
    }
}


p106
#include<stdio.h>
#define N 50
int main()
{
    int arr[N],num,i,j,count;
    printf("Enter the size of array:\n");
    scanf("%d",&num);
    printf("Enter the %d number:\n",num);
    for(i=0; i<num; i++)
    {
        scanf("%d",&arr[i]);
    }
    printf("array is:\n");
    for(i=0; i<num; i++)
    {
        printf("%d\t",arr[i]);
    }
    for(i=0; i<num; i++)
    {
        count=0;
        for (j=0; j<num; j++)
        {
            if(arr[i]==arr[j])
            {
                count++;
            }
        }
            printf("\n%d is repeated %d times",arr[i],count);
    }
}

p107
#include<stdio.h>
#define N 50
int main()
{
    int arr[N][N],max=0,min=0,row,col;
    printf("Enetr the size of array:\n");
    scanf("%d\t%d",&row,&col);
    printf("Enter the elements of array:\n");
    for(int i=0; i<row; i++)
    {
        for(int j=0; j<col; j++)
        {
            scanf("%d",&arr[i][j]);
        }
    }
    printf("matrix:\n");
    for(int i=0; i<row; i++)
    {
        for(int j=0; j<col; j++)
        {
            printf("%d\t",arr[i][j]);
        }
        printf("\n");
    }
    max=arr[0][0];
    min=arr[0][0];
    for(int i=0; i<row; i++)
    {
        for(int j=0; j<col; j++)
        {
            if(max<arr[i][j])
            {
                max=arr[i][j];
            }
            if(min>arr[i][j])
            {
                min=arr[i][j];
            }
        }
    }
    printf("largest number=%d",max);
    printf("\nsmallest number=%d",min);
}


p108
#include<stdio.h>
#define N 50
int main()
{
    int arr[N][N],row,col;
    printf("Enter the size of array");
    scanf("%d\t%d",&row,&col);
    printf("Enter the elements of array:\n");
    for(int i=0; i<row; i++)
    {
        for(int j=0; j<col; j++)
        {
            scanf("%d",&arr[i][j]);
        }
    }
    printf("matrix:\n");
    for(int i=0; i<row; i++)
    {
        for(int j=0; j<col; j++)
        {
            printf("%d\t",arr[i][j]);
        }
        printf("\n");
    }

    printf("reversed matrix:\n");
    for(int i=0; i<row; i++)
    {
        for(int j=col-1; j>=0; j--)
        {
            printf("%d\t",arr[i][j]);
        }
        printf("\n");
    }
}


p109
#include<stdio.h>
#define N 50
int main()
{
    int arr[N][N],ar_odd[N][N],ar_even[N][N];
    printf("Enter the elements of array:\n");
    for(int i=0; i<2; i++)
    {
        for(int j=0; j<2; j++)
        {
            scanf("%d",&arr[i][j]);
        }
    }
    printf("matrix:\n");
    for(int i=0; i<2; i++)
    {
        for(int j=0; j<2; j++)
        {
            printf("%d\t",arr[i][j]);
        }
        printf("\n");
    }

    for(int i=0; i<2; i++)
    {
        for(int j=0; j<2; j++)
        {
            if(arr[i][j]%2==0)
            {
                ar_even[i][j]=arr[i][j];
            }
            if(arr[i][j]%2!=0)
            {
                ar_odd[i][j]=arr[i][j];
            }
        }
    }
    printf("Even matrix:\n");
    for(int i=0; i<2; i++)
    {
        for(int j=0; j<2; j++)
        {
            printf("%d\t",ar_even[i][j]);
        }
        printf("\n");
    }

    printf("odd matrix:\n");
    for(int i=0; i<2; i++)
    {
        for(int j=0; j<2; j++)
        {
            printf("%d\t",ar_odd[i][j]);
        }
        printf("\n");
    }
}


p110
#include<stdio.h>
#define N 50
int main()
{
    int arr[N],pos,i,num;
    printf("Enter the size of array:\n");
    scanf("%d",&num);
    printf("Enter the elements of array:\n");
    for(i=0; i<num; i++)
    {
        scanf("%d",&arr[i]);
    }
    printf("matrix:\n");
    for(i=0; i<num; i++)
    {
        printf("%d\t",arr[i]);
    }
    printf("\nEnter the location of deleting element:\n");
    scanf("%d",&pos);

    if(pos+1>num)
    {
        printf("\ndeleting of element is not possible\n");
    }
    else
    {
        for(i=pos; i<num-1; i++)
        {
            arr[i]=arr[i+1];
        }
        printf("\nafter deleting\n");
        for(i=0; i<num-1; i++)
        {
            printf("%d\t",arr[i]);
        }
    }
}
