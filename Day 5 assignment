int main()
{
int a,b,redmi=12000,realme=21000,oneplus=39000,samsung=50000,apple=70000;
printf("Enter the range");
scanf("%d\n%d",&a,&b);
printf("Mobiles in budget are:\n");
if(redmi>a && redmi<b)
printf("Redmi\n");
if (realme>a && realme<b)
printf("Realme\n");
if(oneplus>a && oneplus<b)
printf("Oneplus\n");
if(samsung>a && samsung<b)
printf("Samsung\n");
if(apple>a && apple<b)
printf("Apple");
}



