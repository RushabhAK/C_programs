   { printf("its a alphabet");}
else if (ch>=48 && ch<=57)
   { printf("its a number");}
else
    {printf("its a special character");}
}



p67
#include<stdio.h>
int main()
{
long int id;
char name[50];
float unit,amt;
printf("ID:");
printf("Name:");
printf("Unit:");
scanf("%d",&id);
scanf("%s",&name);
scanf("%f",&unit);

if(unit<=199)
    {amt=1.20*unit;
    }
else if(unit>=200 && unit<400)
    {amt=(1.20*199)+(1.5*(unit-199));
    }
else if(unit>=400 && unit<=600)
    {amt=(1.20*199)+(1.5*199)+(1.8*(unit-400));
    }
else
    {amt=(1.20*199)+(1.5*199)+(1.8*199)+(2*(unit-599));
    }

if(amt>=400){
    amt=amt+(amt*0.15);
 }
else if(amt<=100){
    amt=100;
 }
printf("Bill=%0.2f",amt);
return 0;

}



p68
#include<stdio.h>
int main()
{
int week;
printf("Enter the week number:");
scanf("%d",&week);
switch(week)
{
case 1:
    printf("Monday");
    break;
case 2:
    printf("Tuesday");
    break;
case 3:
    printf("wednesday");
    break;
case 4:
    printf("Thursday");
     break;
case 5:
    printf("Friday");
     break;
case 6:
    printf("Saturday");
     break;
case 7:
    printf("Sunday");
     break;
Default:
    printf("Invalid number");
}
}



p69
#include<stdio.h>
int main()
{
int day,week,year,num;
printf("Enter the number:");
scanf("%d",&num);
year=num/365;
week=(num%365)/7;
day=(num%365)%7;
printf("days=%d\nweeks=%d\nyear=%d",day,week,year);
}


