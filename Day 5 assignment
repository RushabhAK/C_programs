}



p72
#include<stdio.h>
int main()
{
char operation;
float num1,num2;
printf("Enter the operation:");
scanf("%c",&operation);
printf("Enter the two number:");
scanf("%f%f",&num1,&num2);
switch(operation)
{
case '+':
    printf("%0.2f+%0.2f=%0.2f",num1,num2,num1 + num2);
    break;

case '-':
    printf("%0.2f-%0.2f=%0.2f",num1,num2,num1 - num2);
    break;

case '*':
    printf("%0.2f*%0.2f=%0.2f",num1,num2,num1 * num2);
    break;

case '/':
    printf("%0.2f/%0.2f=%0.2f",num1,num2,num1 / num2);
    break;

default:
    printf("Error");
}

    }
