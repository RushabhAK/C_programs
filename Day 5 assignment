#include<stdio.h>
int main()
{
int num;
printf("Enter the number:");
scanf("%d",&num);
if (num>0)
{
printf("%d is the positive number",num);
}
else if (num<0)
{
    printf("%d is the negative number",num);
}
else
    printf("It's a zero");

}


p57
#include<stdio.h>
int main()
{
char ch;
printf("Enter the character:");
scanf("%c",&ch);
if(ch=='A' || ch=='a')
{
   printf("%c is vowel",ch);
}
else if(ch=='E' || ch=='e')
{
   printf("%c is vowel",ch);
}
else if(ch=='I' || ch=='i')
{
   printf("%c is vowel",ch);
}
else if(ch=='O' || ch=='o')
{
   printf("%c is vowel",ch);
}
else if(ch=='U' || ch=='u')
{
   printf("%c is vowel",ch);
}
else
    printf("%c is consonant",ch);
}



p58
#include<stdio.h>
int main()
{
int year;
printf("Enter the year:");
scanf("%d",&year);
if((year%4==0 || year%400==0) && year%100!=0 )
    {
    printf("%d is the leap year\n",year);
    printf("Next leap year is %d",year+4);
    }
else
   {

    printf("%d is not leap year\n",year);
    if(((year+1)%4==0 || (year+1)%400==0) && (year+1)%100!=0)
    {
        printf("Next leap year is %d",year+1);
    }
    else if(((year+2)%4==0 || (year+2)%400==0) && (year+2)%100!=0)
    {
           printf("Next leap year is %d",year+1);
    }
    else if(((year+3)%4==0 || (year+3)%400==0) && (year+3)%100!=0)
    {
        printf("Next leap year is %d",year+1);
    }
    else
        printf("Next leap year is %d",year+4);
   }

}



p59
#include<stdio.h>
int main()
{
int Ka,En,Hi,Sc,Ma,Avg,total,f=0;
printf("Enter the marks:\n");
scanf("%d\n%d\n%d\n%d\n%d",&Ka,&En,&Hi,&Sc,&Ma);
if(Ka<35)
++f;
if(En<35)
++f;
if(Hi<35)
++f;
if(Sc<35)
++f;
if(Ma<35)
++f;
total=Ka+En+Hi+Sc+Ma;
Avg=total/5;
if(f>0)
{
printf("Fail\n");
printf("failed in %d and passed in %d subjects",f,(5-f));
}
else
{printf("passed in all subjects\n");
    if(Avg>90)
    printf("Distinction");
    else if(Avg>60)
    printf("Firest class");
    else if(Avg>35)
    printf("Second class");
    else
    printf("Fail");
}

}



