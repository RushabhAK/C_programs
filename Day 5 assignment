#include<stdio.h>
int main()
{
char ch1,ch2;
printf("Enter the characters:\n");
scanf("%c\n%c",&ch1,&ch2);
if(ch1==ch2 || ch1==ch2+32 || ch1==ch2-32)
    printf("Both are equal\n");
else
    printf("Both are not equal");
}


