int main()
{
int num1,num2,num3;
printf("Enter the numbers:");
scanf("%d\t%d\t%d",&num1,&num2,&num3);
if (num1>num2 && num1>num3)
printf("%d is the largest number",num1);
else if (num2>num1 && num2>num3)
printf("%d is the largest number",num2);
else
printf("%d is the largest number",num3);
}



p61
#include<stdio.h>
int main()
{
int x,y;
printf("Enter the coordinates:");
scanf("%d%d",&x,&y);
if (x>0 && y>0)
{printf("first quadrant");}
else if (x<0 && y>0)
{printf("second quadrant");}
else if (x<0 && y<0)
{printf("third quadrant");}
else
{printf("fourth quadrant");}

}


p62
#include<stdio.h>
#include<math.h>
int main()
{
int a,b,c,d;
printf("Enter the three values:");
scanf("%d\t%d\t%d",&a,&b,&c);
float r1,r2;
d=(b*b)-4*a*c;
//printf("%d",d);
if (d>0)
{
    r1=(-b+sqrt(d))/(2*a);
    r2=(-b-sqrt(d))/(2*a);
    printf("r1=%0.2f\nr2=%0.2f",r1,r2);
}
else if(d==0)
{
    r1=-b/(2*a);
    r2=-b/(2*a);
    printf("r1=%0.2f\nr2=%0.2f",r1,r2);
}
else
    printf("roots are imaginary");

}



p63
#include<stdio.h>
int main()
{
int temp;
printf("Enter the temperature in centigrade:\n");
scanf("%d",&temp);
if (temp<=0)
    {printf("freezing weather");}
else if(temp>0 && temp<=10)
    {printf("vrey cold weather");}
else if(temp>10 && temp<=20)
    {printf("cold weather");}
else if (temp>20 && temp<=30)
    {printf("normal temp");}
else if(temp>30 && temp<=40)
    {printf("its hot");}
else
    {printf("its very hot");}
}


p64
#include<stdio.h>
int main()
{
int A,B,C;
printf("Enter the triangle lengths:");
scanf("%d\t%d\t%d",&A,&B,&C);
if (A==B && B==C && C==A)
    {printf("Its equilateral triangle");}
else if(A==B || B==C || C==A)
    {printf("Its isosceles triangle");}
else
   {printf("Its sceles triangle");}
}



p65
#include<stdio.h>
int main()
{
int A,B,C;
printf("Enter the triangle lengths:");
scanf("%d\t%d\t%d",&A,&B,&C);
if (A+B>C && A+C>B && B+C>A)
    {printf("Valid");}
else
    printf("Not valid");

}



