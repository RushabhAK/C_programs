p(f)
#include<stdio.h>
int main()
{
double num1=15.2343,num2=23.8763,Res;
Res=num1+num2;
printf("Result=%.20f",Res);
return 0;
}


p(g)
#include<stdio.h>
#include<string.h>
int main()
{
char low,up;
printf("Enter the lower letter:");
scanf("%c",&low);
up=toupper(low);
printf("%c",up);
return 0;
}


p(h)
#include <stdio.h>
int main()
{
int n1;
float n2;
char n3;
long n4;
double n5;
unsigned long n6;
printf(" n1=%d\n n2=%d\n n3=%d\n n4=%d\n n5=%d\n n6=%d\n",sizeof(n1),sizeof(n2),sizeof(n3),sizeof(n4),sizeof(n5),sizeof(n6));
return 0;
}
