p20
#include<stdio.h>
int main()
{
enum days_of_weeks{SUN,MON=4,TUE,WED,THU,FRI,SAT};
enum days_of_weeks day=MON;
printf("%d",day);
}


p21
#include<stdio.h>
#include<stdbool.h>
int main()
{
bool k1=true,k2=0,k3=false,k4=7;
printf("%d\n",k1);
printf("%d\n",k2);
printf("%d\n",k3);
printf("%d",k4);
}

