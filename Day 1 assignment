// program to print a sentence
#include <stdio.h>
int main()
{
  printf("Hi. How are you? ");
  return 0;
}

// Program to print block 'F' using hash(#) character. Also 'C' 
#include <stdio.h>
int main()
{
  printf("#####\n");
  printf("#\n");
  printf("#\n");
  printf("#####\n");
  printf("#\n");
  printf("#\n");
  
   printf("\n    ######\n");
   printf("  ##      ##\n");
   printf(" #\n");
   printf(" #\n");
   printf(" #\n");
   printf(" #\n");
   printf(" #\n");
   printf("  ##      ##\n");
   printf("    ######\n");
   return(0);
}

// Program to read a number and print the same.
#include <stdio.h>
int main()
{   
  int n;
  printf("Enter a number: ");  
  scanf("%d", &n);
  printf("%d", n);
  return 0;
}

// Program to find the square root of a number.
#include <stdio.h>
#include <math.h>
int main()
{
  int n;
  printf("Enter an integer: ");
  scanf("%d", &n);
  printf("%d", sqrt(n));
  return 0;
}

// Program to initialize an integer and print it.
#include <stdio.h>
int main()
{
  int n = 12;
  printf("%d", n);
  return 0;
}

// Program to print the following characters in a reverse way: 'X', 'M', 'L'

#include <stdio.h> 
int main() 
{
  char a[3] = {'X', 'M', 'L'};
  printf("The reverse of %c%c%c is", a[2], a[1], a[0]);
  return(0);
}
